<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.pandalocation" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Panda Location</name>
    <description> Aplicativo de teste com ibeacons - Senai</description>
    <author email="araan666@gmail.com" href="https://github.com/AraanBranco"> Araan Branco </author>
    <author email="renan@agenciabym.com.br" href="https://github.com/renanmariano"> Renan Mariano </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
